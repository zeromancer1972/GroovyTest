<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
	<application>
		<view-handler>
			org.openntf.bstemplate.controller.ControllingViewHandler
		</view-handler>
	</application>
	<managed-bean>
		<managed-bean-name>application</managed-bean-name>
		<managed-bean-class>org.openntf.bstemplate.AppConfig
		</managed-bean-class>
		<managed-bean-scope> application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>navigation</managed-bean-name>
		<managed-bean-class>org.openntf.bstemplate.Navigation
		</managed-bean-class>
		<managed-bean-scope> view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>footer</managed-bean-name>
		<managed-bean-class>org.openntf.bstemplate.NavigationFooter
		</managed-bean-class>
		<managed-bean-scope> view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dojo</managed-bean-name>
		<managed-bean-class>org.openntf.xsp.dojo.Config
		</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>github</managed-bean-name>
		<managed-bean-class>org.openntf.github.xsp.GitHubController
		</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<!--  groovy -->
	<managed-bean>
		<managed-bean-name>groovy</managed-bean-name>
		<managed-bean-class>org.openntf.groovy.CodeRunner
		</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<!-- validators -->
	<validator>
		<validator-id>emailValidator</validator-id>
		<validator-class>org.openntf.bstemplate.EmailValidator
		</validator-class>
	</validator>
	<!-- renderers -->
	<render-kit>
		<renderer>
			<component-family>com.ibm.xsp.Tag</component-family>
			<renderer-type>bootstrap.Panel</renderer-type>
			<renderer-class>org.openntf.bstemplate.renderer.Panel
			</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Panel
			</component-family>
			<renderer-type>bootstrap.Panel</renderer-type>
			<renderer-class>org.openntf.bstemplate.renderer.Panel
			</renderer-class>
		</renderer>
		<renderer>
			<component-family>com.ibm.xsp.Tag</component-family>
			<renderer-type>bootstrap.Alert</renderer-type>
			<renderer-class>org.openntf.bstemplate.renderer.Alert
			</renderer-class>
		</renderer>
		<renderer>
			<component-family>javax.faces.Input
			</component-family>
			<renderer-type>custom.checkbox</renderer-type>
			<renderer-class>org.openntf.bstemplate.renderer.Checkbox
			</renderer-class>
		</renderer>
	</render-kit>
	<!-- IE11 compat mode -->
	<lifecycle>
		<phase-listener>
			org.openntf.bstemplate.UACompatibleHeaderPhaseListener
		</phase-listener>
	</lifecycle>
	<!--
		AUTOGEN-START-BUILDER: Automatically generated by IBM Domino Designer.
		Do not modify.
	-->
	<!--AUTOGEN-END-BUILDER: End of automatically generated section-->
</faces-config>
